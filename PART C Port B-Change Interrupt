#include <xc.h>
#define _XTAL_FREQ 64000000L
#define LED1 LATDbits.LATD0
#define LED2 LATDbits.LATD1
void delay (unsigned int x);
static void interrupt ISR(void);

void main (void)
{
TRISB= ;
TRISDbits.TRISD0=;
TRISDbits.TRISD1=;
INTCONbits.GIE= ;
INTCONbits.PEIE= ;
INTCONbits.RBIE= ;
INTCONbits.RBIF= ;
INTCON2bits.INTEDG0= ;
INTCON2bits.RBIP= ;
INTCON2bits.RBPU= ;

while (1)
{
xxxxxxxxxxx;
delay(100);
}
}

static void interrupt ISR (void)
{
if(INTCONbits.INT0IF)
{
LED2= ;
delay(500);
LED2= ;
delay(500);
LED2= ;
delay(500);
LED2= ;
delay(500);
}
int x = PORTB;
INTCONbits.RBIF= ;
PORTB=x;
}

void delay (unsigned int x)
{
for(;x>0;x--)
__delay_ms(1);
}
